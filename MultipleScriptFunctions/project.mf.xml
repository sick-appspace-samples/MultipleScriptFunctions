<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<manifest>
    <application name="MultipleScriptFunctions">
        <crown name="MultipleScriptFunctions">
            <desc>user script</desc>
            <serves/>
            <crown name="ServingFunction">
                <desc></desc>
                <serves>
                    <function name="printSomething">
                        <desc>No description yet...</desc>
                    </function>
                </serves>
            </crown>
        </crown>
        <meta key="author">AppSpace Sample</meta>
        <meta key="version">1.0</meta>
        <meta key="licensed">false</meta>
        <meta key="priority">low</meta>
        <meta key="read-protected">false</meta>
        <meta key="copy-protected">false</meta>
        <entry path="scripts" default="main.lua">
            <file type="text/x-lua">CallingFunction.lua</file>
            <file type="text/x-lua">main.lua</file>
            <file type="text/x-lua">ServingFunction.lua</file>
        </entry>
    </application>
</manifest>
